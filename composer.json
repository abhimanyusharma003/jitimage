{
  "name": "thapp/jitimage",
  "description": "Just In Time image manipulation",
  "keywords": ["image", "thumbnail", "silex", "laravel"],
  "require": {
    "php":">=5.4.0",
    "lucid/image":"dev-master"
  },
  "license": "MIT",
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "symfony/http-foundation":"~2.6",
    "mockery/mockery": "0.9.*@dev",
    "mikey179/vfsStream": "dev-master",
    "league/flysystem": "~0.4",
    "league/phpunit-coverage-listener":"dev-master"
  },
  "suggest": {
    "league/flysystem": ""
  },
  "authors": [
    {
      "name": "Thomas Appel",
      "email": "mail@thomas-appel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Thapp\\JitImage\\":""
    }
  },
  "repositories": {
    {
      "type": "vcs",
      "url": "https://github.com/lucidphp/image"
    }
  },
  "minimum-stability": "dev"
}
